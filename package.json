{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/server.ts",
  "scripts": {
    "stresstest": "yarn loadtest --rps 400 -c 30 ",
    "prestart": "clear",
    "start": "node lib/server.js",
    "predev": "clear",
    "dev": "NODE_ENV=dev ts-node-dev --respawn --exit-child --quiet --transpile-only -r tsconfig-paths/register src/server.ts",
    "prerun:script": "clear",
    "run:script": "NODE_ENV=dev ts-node-dev --respawn --transpile-only -r tsconfig-paths/register",
    "test": "clear && NODE_ENV=test jest",
    "test:unit": "clear && NODE_ENV=test jest __tests__/unit",
    "unit-test:watch": "clear && NODE_ENV=test jest __tests__/unit --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint-test": "yarn eslint .",
    "build": "yarn build:types && yarn build:js && yarn build:bundle --no-stats && rm -R lib",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".js,.ts\" --source-maps inline",
    "build:bundle": "yarn webpack-cli --no-devtool --progress"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.2"
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.5.0",
    "faker": "5.5.3",
    "jest": "27.4.5",
    "loadtest": "5.1.2",
    "morgan": "1.10.0",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
